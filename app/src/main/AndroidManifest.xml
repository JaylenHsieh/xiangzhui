<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hsieh.xiangzhui">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/ThemeNoActionBar">
        <activity android:name=".ui.OrderActivity"/>
        <activity android:name=".ui.NewMuralActivity" />
        <activity android:name=".ui.MuralDetailActivity" />
        <activity android:name=".ui.MeetingReminderActivity" />
        <activity android:name=".ui.MyCollectionActivity" />
        <activity android:name=".ui.MyDesignActivity" />
        <activity android:name=".ui.WeatherActivity" />
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>